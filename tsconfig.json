{
    "compilerOptions": {
        "module": "ES2022",
        "target": "ES2022",
        "moduleResolution": "NodeNext",
        "lib": [ "ES2022", "DOM" ],
        "noEmit": true,
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "types": [ "vitest/importMeta" ]
    },
    "include": [ ".eslintrc.cjs", "source", "devel" ],
    "exclude": [ "node_modules" ]
}